{
    "openapi": "3.0.1",
    "info": {
        "title": "OpenAPI Audit",
        "version": "v0"
    },
    "servers": [
        {
            "url": "https://localhost:8080/audit",
            "description": "Generated server url"
        }
    ],
    "security": [
        {
            "bearerAuth": []
        }
    ],
    "paths": {
        "/event": {
            "post": {
                "tags": [
                    "open-cdx-rest-audit-controller"
                ],
                "operationId": "event",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/AuditEvent"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/AuditStatus"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Actor": {
                "type": "object",
                "properties": {
                    "identity": {
                        "type": "string"
                    },
                    "role": {
                        "type": "string"
                    },
                    "networkAddress": {
                        "type": "string"
                    },
                    "agentType": {
                        "type": "string",
                        "enum": [
                            "AGENT_TYPE_UNSPECIFIED",
                            "AGENT_TYPE_HUMAN_USER",
                            "AGENT_TYPE_SYSTEM",
                            "AGENT_TYPE_OTHER_ENTITY",
                            "UNRECOGNIZED"
                        ]
                    }
                }
            },
            "AuditEntity": {
                "type": "object",
                "properties": {
                    "patientIdentifier": {
                        "type": "string"
                    },
                    "userIdentifier": {
                        "type": "string"
                    }
                }
            },
            "AuditEvent": {
                "type": "object",
                "properties": {
                    "created": {
                        "$ref": "#/components/schemas/Timestamp"
                    },
                    "eventType": {
                        "type": "string",
                        "enum": [
                            "AUDIT_EVENT_TYPE_UNSPECIFIED",
                            "AUDIT_EVENT_TYPE_USER_LOGIN_SUCCEEDED",
                            "AUDIT_EVENT_TYPE_USER_LOG_OUT",
                            "AUDIT_EVENT_TYPE_USER_LOGIN_FAIL",
                            "AUDIT_EVENT_TYPE_USER_ACCESS_CHANGE",
                            "AUDIT_EVENT_TYPE_USER_PASSWORD_CHANGE",
                            "AUDIT_EVENT_TYPE_SYSTEM_LOGIN_SUCCEEDED",
                            "AUDIT_EVENT_TYPE_SYSTEM_LOG_OUT",
                            "AUDIT_EVENT_TYPE_SYSTEM_LOGIN_FAIL",
                            "AUDIT_EVENT_TYPE_USER_PII_ACCESSED",
                            "AUDIT_EVENT_TYPE_USER_PII_UPDATED",
                            "AUDIT_EVENT_TYPE_USER_PII_CREATED",
                            "AUDIT_EVENT_TYPE_USER_PII_DELETED",
                            "AUDIT_EVENT_TYPE_USER_PHI_ACCESSED",
                            "AUDIT_EVENT_TYPE_USER_PHI_UPDATED",
                            "AUDIT_EVENT_TYPE_USER_PHI_CREATED",
                            "AUDIT_EVENT_TYPE_USER_PHI_DELETED",
                            "AUDIT_EVENT_TYPE_USER_COMMUNICATION",
                            "AUDIT_EVENT_TYPE_CONFIG_CHANGE",
                            "UNRECOGNIZED"
                        ]
                    },
                    "actor": {
                        "$ref": "#/components/schemas/Actor"
                    },
                    "dataObject": {
                        "$ref": "#/components/schemas/DataObject"
                    },
                    "purposeOfUse": {
                        "type": "string"
                    },
                    "auditSource": {
                        "$ref": "#/components/schemas/AuditSource"
                    },
                    "auditEntity": {
                        "$ref": "#/components/schemas/AuditEntity"
                    },
                    "modified": {
                        "$ref": "#/components/schemas/Timestamp"
                    },
                    "creator": {
                        "type": "string"
                    },
                    "modifier": {
                        "type": "string"
                    }
                }
            },
            "AuditSource": {
                "type": "object",
                "properties": {
                    "systemInfo": {
                        "type": "string"
                    },
                    "configuration": {
                        "type": "string"
                    }
                }
            },
            "DataObject": {
                "type": "object",
                "properties": {
                    "resource": {
                        "type": "string"
                    },
                    "data": {
                        "type": "string"
                    },
                    "sensitivity": {
                        "type": "string",
                        "enum": [
                            "SENSITIVITY_LEVEL_UNSPECIFIED",
                            "SENSITIVITY_LEVEL_LOW",
                            "SENSITIVITY_LEVEL_MEDIUM",
                            "SENSITIVITY_LEVEL_HIGH",
                            "UNRECOGNIZED"
                        ]
                    }
                }
            },
            "Timestamp": {
                "type": "object",
                "properties": {
                    "seconds": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "nanos": {
                        "type": "integer",
                        "format": "int32"
                    }
                }
            },
            "AuditStatus": {
                "type": "object",
                "properties": {
                    "success": {
                        "type": "boolean"
                    }
                }
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    }
}
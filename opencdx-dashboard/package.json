{
  "name": "opencdx-dashboard-new",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "startDev": "cross-env NODE_ENV=test next dev",
    "startStatic": "cross-env next start",
    "lint": "eslint . --ext .ts,.tsx -c .eslintrc.json --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cy": "cypress open",
    "build": "next build",
    "generate:api": "npm run generate-api:iam && npm run generate-api:questionnaire",
    "generate-api:classification": "openapi-generator-cli generate -i ./src/api/json/classification.json -g typescript-axios -o ./src/api/classification/ --skip-validate-spec --additional-properties=useSingleRequestParameter=true,withoutPrefixEnums=true,enumNameSuffix=,withSeparateModelsAndApi=true,apiPackage=server,modelPackage=model --openapi-normalizer REFACTOR_ALLOF_WITH_PROPERTIES_ONLY=true",
    "generate-api:iam": "openapi-generator-cli generate -i ./src/api/json/iam.json -g typescript-axios -o ./src/api/iam/ --skip-validate-spec --additional-properties=useSingleRequestParameter=true,withoutPrefixEnums=true,enumNameSuffix=,withSeparateModelsAndApi=true,apiPackage=server,modelPackage=model --openapi-normalizer REFACTOR_ALLOF_WITH_PROPERTIES_ONLY=true",
    "generate-api:questionnaire": "openapi-generator-cli generate -i ./src/api/json/questionnaire.json -g typescript-axios -o ./src/api/questionnaire/ --skip-validate-spec --additional-properties=useSingleRequestParameter=true,withoutPrefixEnums=true,enumNameSuffix=,withSeparateModelsAndApi=true,apiPackage=server,modelPackage=model --openapi-normalizer REFACTOR_ALLOF_WITH_PROPERTIES_ONLY=true"
  },
